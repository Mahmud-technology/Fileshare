<%- include('layout/header') %>
   
   <main class="p-3">
		<div class="nav-container">
			<a href="/">
				<iconify-icon icon="material-symbols:home-rounded"></iconify-icon>
			</a>
			<p>Current Path: <span class="text-muted"><%-storage%></span></p>
		</div>
		<hr />
		  <% for(let i = 0; i < content.length; i++) { %>
		    <a class="folder" href=<%= encodeURI(content[i])  %> >
            <iconify-icon class="icon" icon="material-symbols:folder-sharp"></iconify-icon>
				<%= content[i] %>
		    </a>
		  <% } %>
   </main>
   
<%- include('layout/footer') %>